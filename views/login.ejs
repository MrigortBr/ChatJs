    <title>Login</title>
    <link rel="stylesheet" href="css/loginStyle.css">
    <link rel="stylesheet" href="css/popup.css">
</head>
<body>
    <form action="">
    <div class="login">
        <div class="login-background">
            <div class="login-text">
                <p class="login-text-welcome">Seja bem vindo(a) ao <br> <a>ChatJS</a></p>
            </div>
            <div class="login-infomation">
                <div class="login-infomation-login">
                    <input type="text" name="email" class="login-input" required spellcheck="false">
                    <p class="login-input-text">Digite seu email</p>
                </div>
                <div class="login-infomation-login">
                    <input type="text" name="password" class="login-input" required spellcheck="false">
                    <p class="login-input-text">Digite sua senha</p>
                </div>
                <p class="login-information-register"><a href="/register">NÃ£o tem conta? clique aqui</a> </a></p>

                <button class="login-information-submit" onclick="login()">Login</button>
            </div>
        </div>
    </div>
</form>
